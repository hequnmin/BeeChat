{
  "name": "beechat",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "homepage": "./",
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.11.12",
    "dayjs": "^1.11.9",
    "electron-store": "^8.1.0",
    "ip": "^1.1.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0",
    "react-scripts": "^4.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch": "webpack --config webpack.config.js --watch",
    "package": "electron-packager . BEECHAT --platform=win32 --arch=x64 --out=./release --app-version=1.0.0 --overwrite --asar --icon=./src/assets/image/chat_icon.ico --ignore=node_modules --extra-resource=node_modules/dayjs --extra-resource=node_modules/ip"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^8.0.6",
    "css-loader": "^6.7.3",
    "electron": "^14.2.9",
    "electron-packager": "^15.5.2",
    "electron-reload": "^2.0.0-alpha.1",
    "sass": "^1.62.0",
    "sass-loader": "^13.2.2",
    "socket.io-client": "^4.6.1",
    "style-loader": "^3.3.2",
    "webpack": "^5.76.2",
    "webpack-cli": "^5.1.4"
  },
  "build": {
    "productName": "BEECHAT",
    "appId": "com.BEECHAT.app",
    "directories": {
      "output": "./release"
    },
    "files": [],
    "mac": {
      "icon": "./src/assets/image/chat_icon.ico",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "icon": "./src/assets/image/chat_icon.ico",
      "target": [
        "nsis",
        "zip"
      ],
      "arch": [
        "ia32"
      ]
    }
  },
  "config": {
    "server_ip ": "192.168.1.114"
  }
}
